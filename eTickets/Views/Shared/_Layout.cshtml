                <a class="navbar-brand" asp-area="" asp-controller="Movie" asp-action="Index">ArabSeed</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse  collapse d-sm-inline-flex justify-content-center ">
                    <ul class="nav navbar-nav navbar-center">
                       
                        <li class="nav-item  ">
                            <a class="nav-link  " asp-area="" asp-controller="Movie" asp-action="Index">Movies</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Actor" asp-action="Index">Actors</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Producer" asp-action="Index">Producers</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link " asp-area="" asp-controller="Cinema" asp-action="Index">Cinemas</a>
                        </li>
                        
                    </ul>
                    
                </div>
                 <form class="d-flex text-white">
                      <input class="form-control me-2" type="search" placeholder="Search" aria-label="Search">
                      <button class="btn btn-outline-secondary me-2 text-white" type="submit">Search</button>
                   </form>

                   <button type="button" class="btn btn-outline-success me-2 text-white">Login</button>
            </div>
        </nav>

    </header>
    <div class="container-fluid">
        <main role="main" class="pb-3">
